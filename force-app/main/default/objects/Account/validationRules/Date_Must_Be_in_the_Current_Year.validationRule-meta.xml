<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Date_Must_Be_in_the_Current_Year</fullName>
    <active>false</active>
    <errorConditionFormula>YEAR( SLAExpirationDate__c )  &lt;&gt;  YEAR( TODAY() )</errorConditionFormula>
    <errorDisplayField>SLAExpirationDate__c</errorDisplayField>
    <errorMessage>Same current year needs to be given</errorMessage>
</ValidationRule>
